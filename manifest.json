{
  "manifest_version": 3,
  "name": "STK Cookie Audit",
  "version": "2.1",
  "description": "DÃ©tection DevSecOps des cookies non-HttpOnly.",
  "permissions": [
    "cookies",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "icons": {
    "512": "logoxss.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "48": "logoxss.png",
      "512": "logoxss.png"
    }
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "stk-audit@security.local",
      "strict_min_version": "142.0",
      "data_collection_permissions": {
        "required": ["none"]
      }
    }
  }
}